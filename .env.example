# Environment Variables for Nebraska Treatment Connect
# Copy this file to .env and replace with your actual values

# Google Maps API Key
# Get your API key from: https://console.cloud.google.com/apis/credentials
# Make sure to enable Maps JavaScript API and Geocoding API
# Implement domain restrictions for security
GOOGLE_MAPS_API_KEY=YOUR_ACTUAL_GOOGLE_MAPS_API_KEY_HERE

# Deployment Configuration
ENVIRONMENT=production
DOMAIN=nebraskatreatmentconnect.com

# Feature Flags
ENABLE_PROVIDER_PORTAL=false
ENABLE_STATUS_UPDATES=false
SHOW_DEMO_MESSAGE=false